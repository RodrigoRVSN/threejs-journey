#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rodrigorvsn/workspace/study/threejs-journey/basics/04-animations/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rodrigorvsn/workspace/study/threejs-journey/basics/04-animations/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.75.0_webpack-cli@4.10.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.75.0_webpack-cli@4.10.0/node_modules/webpack/bin/webpack.js" "$@"
fi
