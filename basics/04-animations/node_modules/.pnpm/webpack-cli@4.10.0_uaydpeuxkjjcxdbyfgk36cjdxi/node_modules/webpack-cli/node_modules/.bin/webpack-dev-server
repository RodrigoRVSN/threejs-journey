#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rodrigorvsn/workspace/study/threejs-journey/basics/04-animations/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rodrigorvsn/workspace/study/threejs-journey/basics/04-animations/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-dev-server@4.11.1_pda42hcaj7d62cr262fr632kue/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-dev-server@4.11.1_pda42hcaj7d62cr262fr632kue/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
